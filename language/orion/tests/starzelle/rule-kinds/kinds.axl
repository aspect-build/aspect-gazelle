aspect.gazelle_rule_kind("my_build", {
    "From": "@builder//:a.bzl",
    "NonEmptyAttrs": ["x"],
})

aspect.gazelle_rule_kind("my_test", {
    "From": "@tester//:b.bzl",
    "NonEmptyAttrs": ["y"],
})

aspect.gazelle_rule_kind("should never be used", {
    "From": "@--invalid --label",
})

def declare(ctx):
    ctx.targets.add(
        name = "b",
        kind = "my_build",
        attrs = {
            "x": False,
        },
    )
    ctx.targets.add(
        name = "t",
        kind = "my_test",
        attrs = {
            "y": True,
        },
    )

aspect.orion_extension(
    id = "gen",
    declare = declare,
)
