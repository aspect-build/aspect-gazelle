load("@test//:x.bzl", "props_test")

# gazelle:s foo
# gazelle:s_defaults asdf

# gazelle:sa foo, bar
# gazelle:sa_defaults asdf, fdsa

# gazelle:b foo
# gazelle:b_defaults foo, bar

# gazelle:n 123456
# gazelle:n_defaults foo, 5432

props_test(
    name = "no_defaults",
    b = False,
    n = 123456,
    s = "foo",
    sa = ["foo, bar"],
)

props_test(
    name = "with_defaults",
    b = False,
    n = 123,
    s = "asdf",
    sa = ["asdf, fdsa"],
)
